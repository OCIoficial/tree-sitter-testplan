{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "testplan",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "subtask"
      }
    },
    "subtask": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "subtask_header"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "directive"
              },
              {
                "type": "SYMBOL",
                "name": "command"
              },
              {
                "type": "SYMBOL",
                "name": "_newline"
              }
            ]
          }
        }
      ]
    },
    "subtask_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "STRING",
          "value": "Subtask"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "directive": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "extends"
        },
        {
          "type": "SYMBOL",
          "name": "validator"
        }
      ]
    },
    "extends": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@extends"
        },
        {
          "type": "STRING",
          "value": "subtask"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "validator": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@validator"
        },
        {
          "type": "SYMBOL",
          "name": "arg"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "command": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "group_name"
        },
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "copy"
            },
            {
              "type": "SYMBOL",
              "name": "echo"
            },
            {
              "type": "SYMBOL",
              "name": "generator"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "copy": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "copy"
        },
        {
          "type": "SYMBOL",
          "name": "arg"
        }
      ]
    },
    "echo": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "echo"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "arg"
          }
        }
      ]
    },
    "generator": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "command",
          "content": {
            "type": "SYMBOL",
            "name": "arg"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "arg"
          }
        }
      ]
    },
    "arg": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "word"
        }
      ]
    },
    "word": {
      "type": "PATTERN",
      "value": "[^\\s\"]+"
    },
    "string_literal": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
          },
          {
            "type": "PATTERN",
            "value": "([^\"\\\\]|\\\\.)*"
          },
          {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "STRING",
              "value": "\""
            }
          }
        ]
      }
    },
    "group_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_-]+"
    },
    "_newline": {
      "type": "STRING",
      "value": "\n"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "comment": {
      "type": "PATTERN",
      "value": "#.*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}